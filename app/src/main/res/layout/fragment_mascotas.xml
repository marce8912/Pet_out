<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ic_baseline_pets"
    android:backgroundTint="@color/colorTres"
    tools:context=".fragments.MascotasFragment">

    <fragment

        android:id="@+id/searchMap"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
       android:name="com.google.android.gms.maps.SupportMapFragment"/>
    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floting_ubicacion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="end|bottom"
        android:layout_margin="16dp"
        android:clickable="true"
        android:contentDescription="@string/ubica_tu_mascota"
        android:src="@drawable/ic_baseline_ubicacion"
        app:backgroundTint="@color/colorTres" />




</FrameLayout>